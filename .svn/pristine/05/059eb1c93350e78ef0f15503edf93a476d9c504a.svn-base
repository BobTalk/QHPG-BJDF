    <!--验证文本框-->
    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>验证文本框</legend>
    </fieldset>

    <div class="layui-form-item">
        <!--验证必填项-->
        <div class="layui-inline">
            <span class="dataRequired"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="username" lay-verify="required" autocomplete="off" placeholder="验证必填项"
                           class="layui-input dataInput ">
                </div>
            </div>
        </div>
        <!--请输入手机号-->
        <div class="layui-inline">
            <span class="dataPhone"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="phone" lay-verify="phone" autocomplete="off" placeholder="请输入手机号"
                           class="layui-input dataInput ">
                </div>
            </div>
        </div>

        <!--请输入邮箱-->
        <div class="layui-inline">
            <span class="dataEmail"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="email" lay-verify="email" autocomplete="off" placeholder="请输入邮箱"
                           class="layui-input dataInput ">
                </div>
            </div>
        </div>

        <!--多规则验证邮箱或手机号-->
        <div class="layui-inline">
            <span class="dataPhone"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="number" lay-verify="email|number" autocomplete="off"
                           placeholder="多规则验证邮箱或手机号" class="layui-input dataInput ">
                </div>
            </div>
        </div>
        <!--请选择日期-->
        <div class="layui-inline">
            <span class="dataDate"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="date" id="date" lay-verify="date" autocomplete="off" placeholder="请选择日期"
                           class="layui-input dataInput " onclick="layui.laydate({elem: this})">
                </div>
            </div>
        </div>
        <!--请输入链接地址-->
        <div class="layui-inline">
            <span class="dataUrl"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="tel" name="url" lay-verify="url" autocomplete="off" placeholder="请输入链接地址"
                           class="layui-input dataInput ">
                </div>
            </div>
        </div>
        <!--请输入身份证-->
        <div class="layui-inline">
            <span class="dataIdentity"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="identity" lay-verify="identity" autocomplete="off" placeholder="请输入身份证"
                           class="layui-input dataInput ">
                </div>
            </div>
        </div>
    </div>
    <!--请输入密码-->
