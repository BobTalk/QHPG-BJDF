        <legend>文本框集合</legend>
    </fieldset>

    <!--普通输入框-->
    <div class="layui-form-item">
        <div class="dataInputBox">
            <p class="dataInputHeader"></p>
            <div class="layui-input-inline">
                <input type="text" name="username" placeholder="请输入" autocomplete="off" class="layui-input  dataInput">
            </div>
        </div>
    </div>

    <!--较短输入框-->
    <div class="layui-form-item">
        <div class="dataInputBox">
            <p class="dataInputHeader"></p>
            <div class="layui-input-inline">
                <input type="text" name="username" placeholder="短输入框" autocomplete="off"
                       class="layui-input dataInput ">
            </div>
        </div>
    </div>

    <!--较长输入框-->
    <div class="layui-form-item">
        <div class="dataInputBox">
            <p class="dataInputHeader"></p>
            <div class="">
                <input type="text" name="title" autocomplete="off" placeholder="请输入标题"
                       class="layui-input dataInput dataInputLong">
            </div>
        </div>
    </div>


    <fieldset class="layui-elem-field layui-field-title" style="margin-top: 20px;">
        <legend>日期控件</legend>
    </fieldset>
    <div class="layui-form-item">
        <!--请选择日期-->
        <div class="layui-inline">
            <span class="dataDate"></span>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="">
                    <input type="text" name="date"  lay-verify="date" autocomplete="off" placeholder="请选择日期"
                           class="layui-input dataInput " onclick="layui.laydate({elem: this})">
                </div>
            </div>
        </div>

        <div class="layui-inline">
            <label class="layui-form-label">原生日期</label>
            <div class="layui-input-block">
                <input type="date" name="date1"  autocomplete="off" class=" layui-input" onclick="layui.laydate({elem: this})">
            </div>
        </div>
        <!--有起止的日期插件-->
        <div class="layui-inline">
            <span class="dataDate"></span>
            <div class="dataInputBox dataDateBox">
                <p class="dataInputHeader"></p>
                <div class=" fl">
                    <input type="text" name="date" lay-verify="date" autocomplete="off" placeholder="请选择开始日期"
                           id="LAY_demorange_s" class="layui-input dataInput dataDateInput ">
                </div>
            </div>
            <div class="dataInputBox">
                <p class="dataInputHeader"></p>
                <div class="fl">
                    <input type="text" name="date" lay-verify="date" autocomplete="off" placeholder="请选择截止日期"
                           id="LAY_demorange_e" class="layui-input dataInput dataDateInput">
                </div>
            </div>
        </div>
