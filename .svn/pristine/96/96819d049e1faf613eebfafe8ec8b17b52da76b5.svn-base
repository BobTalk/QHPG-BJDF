.layui-nav-tree .layui-nav-bar{width: 0px;}

.layui-nav-tree .layui-nav-child dd.layui-this, .layui-nav-tree .layui-this, .layui-nav-tree .layui-this>a, .layui-nav-tree .layui-this>a:hover { background-color: #4fc3a5;  color: #fff;  }
